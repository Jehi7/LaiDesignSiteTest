{
    "presets": ["next/babel"],
    "plugins": [
        [
        "module-resolver",
        {
            "root": ["./"],
            "alias": {
            "~components": "./src/components",   //ignore this
            "underscore": "lodash"
            }
        }
        ],
        [
        "styled-components",
        { "ssr": true, "displayName": true, "preprocess": false }
        ]
    ]
}